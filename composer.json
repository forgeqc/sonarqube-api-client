{
  "name": "forgeqc/sonarqube-api-client",
  "version": "1.1.0",
  "type": "library",
  "description": "A simple PHP client for sonarqube and sonarcloud.io API",
  "keywords": ["sonarqube","sonarcloud","api","client"],
  "license": "MIT",
  "require": {
      "guzzlehttp/guzzle": "^6.3",
      "kevinrob/guzzle-cache-middleware": "^3.2",
      "doctrine/cache": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ForgeQC\\SonarqubeApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\ForgeQC\\SonarqubeApiClient\\": "tests/"
    }
  },
  "archive": {
    "exclude": ["vendor/", "tests/", ".gitignore"]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "vlucas/phpdotenv": "^3.4"
  }
}
